{
  "name": "isro-api",
  "version": "v1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "crawl": "python -m crawlers.run_all",
    "crawl:spacecraft": "python -m crawlers.spacecraft_missions",
    "crawl:launches": "python -m crawlers.launch_missions",
